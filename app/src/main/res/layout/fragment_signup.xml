<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignupFragment">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="446dp"
        android:layout_height="777dp"
        android:src="@drawable/ic_signup2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/fragemnt_signup_comapny"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="144dp"
        app:layout_constraintStart_toEndOf="@+id/fragment_signup_influencer"
        tools:layout_editor_absoluteY="171dp" />

    <CheckBox
        android:id="@+id/fragment_signup_influencer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        app:layout_constraintEnd_toStartOf="@+id/fragemnt_signup_comapny"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="171dp" />

    <TextView
        android:id="@+id/fragemnt_signup_username"
        android:layout_width="216dp"
        android:layout_height="28dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragemnt_signup_comapny" />

    <TextView
        android:id="@+id/fragemnt_signup_email"
        android:layout_width="216dp"
        android:layout_height="28dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragemnt_signup_username" />

    <TextView
        android:id="@+id/fragemnt_signup_password"
        android:layout_width="216dp"
        android:layout_height="28dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragemnt_signup_email" />

    <TextView
        android:id="@+id/fragemnt_signup_age"
        android:layout_width="216dp"
        android:layout_height="28dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragemnt_signup_password" />

    <Spinner
        android:id="@+id/fragemnt_signup_gender"
        android:layout_width="267dp"
        android:layout_height="37dp"
        android:layout_marginTop="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.812"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragemnt_signup_age" />

    <Button
        android:id="@+id/fragemnt_signup_uploadbtn"
        android:layout_width="108dp"
        android:layout_height="46dp"
        android:backgroundTint="#4c266a"
        android:text="Upload"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="581dp" />

    <Button
        android:id="@+id/fragemnt_signup_backbtn"
        android:layout_width="115dp"
        android:layout_height="48dp"
        android:layout_marginTop="28dp"
        android:backgroundTint="#4c266a"
        android:text="back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.168"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragemnt_signup_uploadbtn" />

    <Button
        android:id="@+id/fragemnt_signup_continuebtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:backgroundTint="#4c266a"
        android:text="continue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.639"
        app:layout_constraintStart_toEndOf="@+id/fragemnt_signup_backbtn"
        app:layout_constraintTop_toBottomOf="@+id/fragemnt_signup_uploadbtn" />
</androidx.constraintlayout.widget.ConstraintLayout>